{
  "name": "showcase",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run dev",
    "dev": "ng serve --open",
    "startup": "npm run local",
    "local": "ng serve --open --ssl true --ssl-key util/ssl/server.key --ssl-cert util/ssl/server.crt",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/model": "^8.1.0",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "~8.2.14",
    "@types/jwt-decode": "^2.2.1",
    "angular-calendar": "^0.27.8",
    "core-js": "^3.1.3",
    "date-fns": "^1.30.1",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "mini-dash": "^0.2.0",
    "moment": "^2.22.2",
    "ng-http-loader": "^5.1.0",
    "ng-snotify": "^4.3.1",
    "ngx-api-utils": "^1.0.8",
    "rxjs": "^6.5.0",
    "rxjs-compat": "^6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/core": "^8.3.24",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@testing-library/angular": "^8.2.0",
    "@types/jest": "^25.1.2",
    "@types/lodash": "^4.14.136",
    "@types/node": "^13.7.0",
    "angular-oauth2-oidc": "^5.0.2",
    "codelyzer": "^5.0.0",
    "jest": "^25.1.0",
    "jest-preset-angular": "^8.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.0.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./setupjest.ts"
    ],
    "testPathIgnorePatterns": [
      "./dist",
      "./node_modules",
      "./utils",
      "./src/test.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "./node_modules/jest-preset-angular/InlineHtmlStripStylesTransformer"
        ]
      }
    }
  }
}
